<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  RewriteCond %{HTTP_HOST} ^centropsicologicocontigovoy\.com$ [NC]
  RewriteRule ^(.*)$ https://www.centropsicologicocontigovoy.com/$1 [L,R=301]

  RewriteCond %{REQUEST_URI} ^/apicontigovoy/
  RewriteRule ^(.*)$ - [L]

  RewriteRule ^index.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule . /index.html [L]
</IfModule>